{
	"request": [
		{
			"enable": true,
			"name": "bing-cn-to-www",
			"ruleType": "redirect",
			"matchType": "url",
			"pattern": "https://www.bing.com/?mkt=zh-CN",
			"exclude": "",
			"group": "bing",
			"isFunction": false,
			"action": "redirect",
			"to": "https://www.bing.com"
		}
	],
	"sendHeader": [
		{
			"enable": true,
			"name": "bing",
			"ruleType": "modifySendHeader",
			"matchType": "regexp",
			"pattern": "^http(s?)://(.*).bing\\.com/(.*)",
			"exclude": "",
			"group": "bing",
			"isFunction": false,
			"action": {
				"name": "x-forwarded-for",
				"value": "8.8.8.8"
			}
		},
		{
			"enable": true,
			"name": "Newbing",
			"ruleType": "modifySendHeader",
			"matchType": "regexp",
			"pattern": "^http(s?)://(.*).bing\\.com/(.*)",
			"exclude": "",
			"group": "bing",
			"isFunction": false,
			"action": {
				"name": "user-agent",
				"value": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/110.0.0.0 Safari/537.36 Edg/110.0.1587.41"
			}
		}
	],
	"receiveHeader": [],
	"receiveBody": []
}